<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane minHeight="-Infinity" prefHeight="204.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.DeleteController">
   <children>
      <Label fx:id="labelAreUSure" alignment="CENTER" layoutX="17.0" layoutY="14.0" prefHeight="80.0" prefWidth="315.0" text="    Are you sure you want to delete this student?" wrapText="true">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Pane prefHeight="16.0" prefWidth="350.0" style="-fx-background-color: #FEA360;" />
      <Pane layoutY="190.0" prefHeight="16.0" prefWidth="350.0" style="-fx-background-color: #FEA360;" />
      <Label fx:id="labelDeleted" alignment="CENTER" layoutY="88.0" prefHeight="49.0" prefWidth="350.0" textAlignment="CENTER">
         <font>
            <Font size="17.0" />
         </font>
       <TextFlow textAlignment="CENTER" />
      </Label>
      <Button fx:id="buttonCancelDelete" layoutX="207.0" layoutY="147.0" mnemonicParsing="false" onAction="#handleCancelDelete" style="-fx-background-color: #D4643F;" text="No, cancel delete" textFill="#f5f3f3">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="buttonYesDelete" layoutX="29.0" layoutY="147.0" mnemonicParsing="false" style="-fx-background-color: #D4643F;" text="Yes, delete student" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
