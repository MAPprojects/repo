<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Pagination?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="607.0" prefWidth="838.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.StudentiController">
   <children>
      <Pagination fx:id="pagination" prefHeight="504.0" prefWidth="567.0" />
      <TableView fx:id="tableView" prefHeight="450.0" prefWidth="567.0">
        <columns>
          <TableColumn fx:id="colNume" prefWidth="107.0" text="Nume" />
          <TableColumn fx:id="colGrupa" prefWidth="106.0" text="Grupa" />
            <TableColumn fx:id="colEmail" prefWidth="115.0" text="Email" />
            <TableColumn fx:id="colCadruDidactic" prefWidth="112.0" text="Profesor" />
            <TableColumn fx:id="deleteColumn" prefWidth="125.0" text="Actions" />
        </columns>
      </TableView>
      <Button fx:id="addButton" layoutX="571.0" layoutY="32.0" mnemonicParsing="false" onAction="#addButtonHandler" prefHeight="25.0" prefWidth="95.0" text="Add" />
      <Button fx:id="clearButton" layoutX="571.0" layoutY="82.0" mnemonicParsing="false" onAction="#clearTFields" prefHeight="25.0" prefWidth="95.0" text="Clear" />
      <TextField fx:id="id" layoutX="666.0" layoutY="32.0" />
      <TextField fx:id="nume" layoutX="666.0" layoutY="57.0" onAction="#onEnterNume" />
      <TextField fx:id="grupa" layoutX="666.0" layoutY="82.0" onAction="#onEnterGrupa" />
      <TextField fx:id="email" layoutX="666.0" layoutY="107.0" onAction="#onEnterEmail" />
      <TextField fx:id="profesor" layoutX="666.0" layoutY="132.0" />
      <TextField fx:id="filter" layoutX="695.0" layoutY="225.0" prefHeight="25.0" prefWidth="120.0" />
      <ComboBox fx:id="comboStudenti" layoutX="575.0" layoutY="225.0" onAction="#filtrareStudHandler" prefHeight="25.0" prefWidth="120.0" />
      <Button fx:id="updateButton" layoutX="571.0" layoutY="57.0" mnemonicParsing="false" onAction="#updateButtonHandler" prefHeight="25.0" prefWidth="95.0" text="Modifica" />
   </children>
</AnchorPane>
