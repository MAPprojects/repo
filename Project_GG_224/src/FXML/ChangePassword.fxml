<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="anchorpane" prefHeight="470.0" prefWidth="305.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FXML.ChangePassword">
   <children>
      <PasswordField fx:id="tf_old" layoutX="78.0" layoutY="121.0" />
      <PasswordField fx:id="tf_new" layoutX="78.0" layoutY="223.0" />
      <PasswordField fx:id="tf_new2" layoutX="78.0" layoutY="318.0" />
      <Button fx:id="b_ok" layoutX="136.0" layoutY="392.0" mnemonicParsing="false" onAction="#handleChange" text="OK" />
      <Label layoutX="80.0" layoutY="79.0" prefHeight="17.0" prefWidth="149.0" text="Old Password:">
         <font>
            <Font name="Bookman Old Style" size="19.0" />
         </font>
      </Label>
      <Label layoutX="78.0" layoutY="190.0" prefHeight="17.0" prefWidth="149.0" text="New Password">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Label layoutX="80.0" layoutY="278.0" prefHeight="17.0" prefWidth="149.0" text="New Password">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
   </children>
   <stylesheets>
      <URL value="@img/button.css" />
      <URL value="@cssSheet/thema.css" />
