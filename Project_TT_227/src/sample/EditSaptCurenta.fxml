<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane maxHeight="144.0" maxWidth="400.0" minHeight="142.0" minWidth="320.0" prefHeight="143.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.EditSaptCurenta">
   <bottom>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="label1" text="Label" textFill="WHITE">
               <font>
                  <Font name="Comic Sans MS Bold Italic" size="13.0" />
               </font>
            </Label>
            <Region HBox.hgrow="ALWAYS" />
            <Label fx:id="label2" text="Label" textFill="WHITE">
               <font>
                  <Font name="Comic Sans MS Bold Italic" size="13.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </bottom>
   <center>
      <VBox alignment="TOP_RIGHT" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="txt1" onMouseClicked="#reset" promptText="Saptamana curenta">
               <font>
                  <Font name="Comic Sans MS Bold Italic" size="14.0" />
               </font>
            </TextField>
          <Button fx:id="btnOK" mnemonicParsing="false" onMouseClicked="#updateSaptamanaCurenta" style="-fx-background-color: rgb(0, 80, 100);" text="OK" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Comic Sans MS Bold" size="12.0" />
               </font>
               <effect>
                  <DropShadow color="#0000007f" height="1.95" offsetY="3.0" radius="0.0" spread="0.02" width="0.0" />
               </effect>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
      </VBox>
   </center>
   <top>
      <Region prefHeight="43.0" prefWidth="320.0" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
