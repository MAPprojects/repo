          <TableColumn fx:id="columnId" prefWidth="91.0" text="Id">
              <cellValueFactory>
                  <PropertyValueFactory property="idHomework" />
              </cellValueFactory>
          </TableColumn>

          <TableColumn prefWidth="91.0" text="Deadline">
              <cellValueFactory>
                  <PropertyValueFactory property="deadline" />
