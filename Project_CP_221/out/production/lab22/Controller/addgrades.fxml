   <stylesheets>
       <URL value="@../Controller/design.css">
       </URL>
   </stylesheets>

    <children>
      <TableView fx:id="tableViewHw" editable="true" layoutX="96.0" layoutY="34.0" prefHeight="302.0" prefWidth="365.0">
