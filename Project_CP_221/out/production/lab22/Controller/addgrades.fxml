          </TableColumn>

       <TableColumn prefWidth="78.0" text="Description">
           <cellValueFactory>
               <PropertyValueFactory property="description" />
           </cellValueFactory>
       </TableColumn>
              <TableColumn fx:id="columnGrade" editable="true" onEditCommit="#handlerOnEdit" prefWidth="104.0" text="Grade">
              </TableColumn>
          </columns>
      </TableView>
   </children>
