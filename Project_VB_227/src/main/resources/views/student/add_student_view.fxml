                            <graphic>
                                <ImageView fitHeight="39.0" fitWidth="47.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@add_student_accept.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <tooltip>
                                <Tooltip text="Adauga"/>
                            </tooltip>
                        </Button>
                        <Button fx:id="exitDialogAddStudent" contentDisplay="TOP" layoutX="90.0" layoutY="4.0"
                                mnemonicParsing="false" onMouseClicked="#exitDialogAddStudentHandler" prefHeight="59.0"
                                prefWidth="69.0">
                            <graphic>
                                <ImageView fitHeight="39.0" fitWidth="47.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@add_stundet_declien.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <tooltip>
                                <Tooltip text="Iesire"/>
                            </tooltip>
                        </Button>
                    </children>
                </Pane>
            </bottom>
        </BorderPane>
