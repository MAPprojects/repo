            fx:controller="controller.AddStudentController">
    <children>
        <Pane layoutX="209.0" layoutY="6.0" prefHeight="357.0" prefWidth="104.0">
            <children>
                <ImageView fitHeight="397.0" fitWidth="401.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@color_splash.png"/>
                    </image>
                </ImageView>
            </children>
        </Pane>
        <BorderPane fx:id="addStudentDialogBorderPane" layoutX="42.0" layoutY="58.0" prefHeight="500.0"
                    prefWidth="749.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <center>
                <GridPane prefHeight="328.0" prefWidth="630.0" BorderPane.alignment="CENTER">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
