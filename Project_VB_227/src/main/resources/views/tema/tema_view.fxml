<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="temaRootAnchorPane" prefHeight="650.0" prefWidth="1000.0"
            stylesheets="@tema.css" xmlns="http://javafx.com/javafx/9.0.1" fx:controller="controller.TemaController">
    <children>
        <BorderPane fx:id="temeBorderPane" layoutX="72.0" layoutY="63.0" onMouseClicked="#borderPaneHandler"
                    prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <left>
                <Pane fx:id="paneButoaneMeniu" prefHeight="425.0" prefWidth="126.0" BorderPane.alignment="CENTER">
                    <children>
                        <Button fx:id="temaBackButton" contentDisplay="TOP" layoutX="32.0" layoutY="43.0"
                                mnemonicParsing="false" onMouseClicked="#backButtonHandler" prefHeight="59.0"
                                prefWidth="68.0" scaleX="1.5" scaleY="1.5" text="Inapoi">
                            <graphic>
                                <ImageView fitHeight="44.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../menu_utils_png/back_icon.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button fx:id="addButton" contentDisplay="TOP" layoutX="15.0" layoutY="143.0"
                                mnemonicParsing="false" onMouseClicked="#addButtonHandler" prefHeight="96.0"
                                prefWidth="106.0" text="Adauga">
                            <font>
                                <Font size="18.0"/>
                            </font>
                            <graphic>
                                <ImageView fitHeight="77.0" fitWidth="73.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../menu_utils_png/add_icon.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button fx:id="stergeButton" contentDisplay="TOP" layoutX="10.0" layoutY="370.0"
                                mnemonicParsing="false" onMouseClicked="#stergeTemaHandler" prefHeight="108.0"
                                prefWidth="108.0" text="Sterge">
                            <graphic>
                                <ImageView fitHeight="65.0" fitWidth="67.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../menu_utils_png/delete-button-png-28567.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <font>
                                <Font size="18.0"/>
                            </font>
                        </Button>
                        <Button fx:id="prelungireButton" contentDisplay="TOP" layoutX="10.0" layoutY="264.0"
                                mnemonicParsing="false" onMouseClicked="#prelungireButtonHandler" prefHeight="102.0"
                                prefWidth="109.0" text="Prelungire">
                            <font>
                                <Font size="18.0"/>
                            </font>
                            <graphic>
                                <ImageView fitHeight="59.0" fitWidth="71.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../menu_utils_png/clock_icon.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                    </children>
                </Pane>
            </left>
            <right>
                <Pane prefHeight="650.0" prefWidth="424.0" BorderPane.alignment="CENTER">
                    <children>
                        <ImageView fitHeight="550.0" fitWidth="620.0" layoutX="-140.0" layoutY="-17.0"
                                   pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@jtree_png.png"/>
                            </image>
                        </ImageView>
                        <Pane fx:id="userOptionsPane" layoutX="8.0" layoutY="406.0" prefHeight="238.0"
                              prefWidth="395.0">
                            <children>
                                <Text fx:id="textCerinta" layoutX="66.0" layoutY="54.0" strokeType="OUTSIDE"
                                      strokeWidth="0.0" text="CERINTA:" wrappingWidth="176.8033447265625"/>
                                <TextArea fx:id="cerintaTextArea" layoutX="67.0" layoutY="71.0" prefHeight="96.0"
                                          prefWidth="255.0" wrapText="true"/>
                                <Button fx:id="filtreazaDupaCerintaButton" layoutX="80.0" layoutY="190.0"
                                        mnemonicParsing="false" onMouseClicked="#filtreazaTemaHandler" prefHeight="28.0"
                                        prefWidth="98.0" text="FILTREAZA" textAlignment="CENTER"/>
                                <ComboBox fx:id="termenPredareComboBox" layoutX="155.0" layoutY="37.0"
                                          onMouseClicked="#populateTermenPredareCombo" prefHeight="25.0"
                                          prefWidth="167.0" promptText="TERMEN DE PREDARE"/>
                                <Button fx:id="stergeFiltreTema" layoutX="202.0" layoutY="190.0" mnemonicParsing="false"
                                        onMouseClicked="#stergeFiltreHandler" prefHeight="28.0" prefWidth="117.0"
                                        text="STERGE FILTRE"/>
                            </children>
                        </Pane>
                        <Pane fx:id="paneMesajeEroare" layoutX="2.0" layoutY="340.0" prefHeight="45.0"
                              prefWidth="250.0">
                            <children>
                                <ImageView fitHeight="34.0" fitWidth="45.0" layoutX="8.0" layoutY="8.0"
                                           pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../systemConfiguration/warning-icon-png-2745.png"/>
                                    </image>
                                </ImageView>
                                <Text fx:id="mesajEroarePrelungire" fill="RED" layoutX="43.0" layoutY="28.0"
                                      strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="206.1366958618164">
                                    <font>
                                        <Font name="System Bold" size="12.0"/>
                                    </font>
                                </Text>
                            </children>
                        </Pane>
                    </children>
                </Pane>
            </right>
        </BorderPane>
        <Pane fx:id="prelungireInputPane" layoutX="40.0" layoutY="340.0" prefHeight="74.0" prefWidth="201.0">
            <children>
                <TextField fx:id="prelungireTextField" layoutX="12.0" layoutY="7.0" onKeyPressed="#prelungireHandler"
                           prefHeight="37.0" prefWidth="149.0" promptText="Termen nou de predare"/>
            </children>
        </Pane>
        <TableView fx:id="tableTeme" onMouseClicked="#clickTableHandler" prefHeight="600.0" prefWidth="81.0">
            <columns>
                <TableColumn fx:id="coloanaCerinta" prefWidth="211.0" text="CERINTA"/>
                <TableColumn fx:id="coloanaTermenPredare" prefWidth="283.0" text="TERMEN PREDARE"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </children>
</AnchorPane>
