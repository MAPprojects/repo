<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>

<?import javafx.scene.control.cell.PropertyValueFactory?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Graphics.Students.StudentsController"
            prefHeight="500.0" prefWidth="1000.0"
            stylesheets="/Graphics/Styles/students.css"
            styleClass="pane"
>
    <VBox>
        <ComboBox styleClass="admin-button" fx:id="settingsBox" />
        <Pane prefHeight="50d"></Pane>
        <HBox alignment="CENTER">
            <Pane prefWidth="300.0"></Pane>
            <TableView  fx:id="tableView" styleClass="table">
                <columns>
                    <TableColumn fx:id="taskColumn" text="Task">
                        <cellValueFactory><PropertyValueFactory property="task"/></cellValueFactory>
                    </TableColumn>
                    <TableColumn fx:id="gradeColumn" text="Grade">
                        <cellValueFactory><PropertyValueFactory property="grade"/></cellValueFactory>
                    </TableColumn>
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                </columnResizePolicy>
            </TableView>

        </HBox>
        <HBox alignment="CENTER">
            <Pane prefWidth="300.0"></Pane>
            <Pagination fx:id="pagination" stylesheets="/Graphics/Styles/pagination.css"></Pagination>
        </HBox>

    </VBox>
</AnchorPane>
